              }
           ]
        },
        "HarmonizationVariation":{
           "properties":{
              "string_to_harmonize":{
                 "type":"string",
                 "title":"String To Harmonize"
              },
              "data_commons_id":{
                 "type":"integer",
                 "title":"Data Commons Id"
              },
              "cde_id":{
                 "type":"integer",
                 "title":"Cde Id"
              },
              "cde_version_id":{
                 "type":"string",
                 "title":"Cde Version Id"
              }
           },
           "type":"object",
           "required":[
              "string_to_harmonize",
              "data_commons_id",
              "cde_id",
              "cde_version_id"
           ],
           "title":"HarmonizationVariation",
           "examples":[
              {
                 "cde_id":100,
                 "cde_version_id":-1,
                 "data_commons_id":-1,
                 "string_to_harmonize":"NB"
              }
           ]
        },
        "HarmonizedTerm":{
           "properties":{
              "option":{
                 "type":"string",
                 "title":"Option",
                 "default":""
              },
              "confidence_score":{
                 "type":"number",
                 "title":"Confidence Score",
                 "default":0.0
              }
           },
           "type":"object",
           "title":"HarmonizedTerm"
        },
        "ValidationError":{
           "properties":{
              "loc":{
                 "items":{
                    "anyOf":[
